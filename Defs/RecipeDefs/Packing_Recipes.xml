<RecipeDefs>



   <!-- =======Minerals======= -->
   <RecipeDef>
      <defName>MakeSteelCrate</defName>
      <label>Steel Crate (75)</label>
      <description>Packs 75 steel pieces into a crate.</description>
      <jobString>Packing heat.</jobString>
      <workAmount>100</workAmount>
      <workSpeedStat>SmeltingSpeed</workSpeedStat>
      <effectWorking>ConstructMetal</effectWorking>
      <ingredients>
         <li>
            <filter>
               <thingDefs>
                  <li>Steel</li>
               </thingDefs>
            </filter>
            <count>75</count>
         </li>
         <li>
            <filter>
               <thingDefs>
                  <li>KMSteelPlate</li>
               </thingDefs>
            </filter>
            <count>6</count>
         </li>
      </ingredients>
      <products>
         <SteelCrate>1</SteelCrate>
      </products>
      <fixedIngredientFilter>
         <thingDefs>
            <li>KMSteelPlate</li>
            <li>Steel</li>
         </thingDefs>
      </fixedIngredientFilter>
   </RecipeDef>

